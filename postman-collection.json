            },
            "url": {
              "raw": "{{base_url}}/api/login",
              "host": ["{{base_url}}"],
              "path": ["api", "login"]
            }
          }
        },
        {
          "name": "Login - Rate Limiting Test",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "// After 10 requests in 1 minute, should get 429",
                  "pm.test(\"Rate limiting works\", function () {",
                  "    // This test should be run 11 times rapidly",
                  "    if (pm.info.iteration >= 10) {",
                  "        pm.response.to.have.status(429);",
                  "    }",
                  "});"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
